    "start:dev": "nodemon src/server/index",
    "start": "ENV=production PORT=4000 node src/server/index",
    "lint": "eslint src/frontend/ --ext .js --ext .jsx --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
