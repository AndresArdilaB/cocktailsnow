  "description": "Prueba tecnica para Platzi hecha por Andres Ardila, https://andresardila.me",
  "main": "index.js",
  "scripts": {
    "test": "jest  --verbose ./src",
    "test:e2e": "cypress run --project ./test",
    "test:e2e:dev": "cypress open --project ./test",
    "test:watch": "jest --watch",
    "test:update": "jest --updateSnapshot ./src",
    "build": "ENV=production PORT=4000 webpack-cli --config webpack.config.js",
